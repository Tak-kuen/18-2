import javax.swing.*;
import java.awt.*;

class MyFrame1 extends JFrame{
	public MyFrame1() {
		setSize(600,450);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setTitle("title");
		
		JPanel panel = new JPanel();
		JLabel label = new JLabel("Sd");
		JButton bt1 = new JButton("dd1");
		JButton bt2 = new JButton("dd2");
		JButton bt3 = new JButton("dd3");
		JTextField textField1 = new JTextField(20);
		
		panel.add(label);
		panel.add(bt1);
		panel.add(bt2);
		panel.add(bt3);
		this.add(panel);
		setVisible(true);
	}
}
