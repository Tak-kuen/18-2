class Box<T> {
	private T data;
	void set(T o) { data=o;	}
	T get() { return data;	}
}



public class WDB {
	public static void main(String[] args) {
//	//	String data = "홍길동";
//		Integer data = 10;
//		Box box = new Box();
//		box.set(data);
//		data = (Integer) box.get();
//		String a = (String)box.get();	//오류발생
//		System.out.println(a);
		Box<Integer> bi = new Box<>();
		Box<String> bs = new Box<>();
		
		bi.set(11);
		bs.set("홍길동");
	}
}
