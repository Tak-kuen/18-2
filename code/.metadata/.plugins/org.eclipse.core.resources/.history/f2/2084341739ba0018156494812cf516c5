import javax.swing.*;
class MyFrame2 {
	public MyFrame2() extends JFrame {
		setSize(600,450);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setTitle("title");
		
		JPanel panel = new JPanel();
		JLabel label = new JLabel("Sd");
		JButton bt1 = new JButton("dd1");
		JButton bt2 = new JButton("dd2");
		JButton bt3 = new JButton("dd3");
		JTextField textField1 = new JTextField(10);
		JTextField textField2 = new JTextField(10);
		textField1.setText("바꾸기가능");
		textField2.setText("바꾸기ㄴㄴ");
		textField2.setEditable(false);
		
		
		
		panel.add(label);
		panel.add(bt1);
		panel.add(bt2);
		panel.add(bt3);
		panel.add(textField1);
		panel.add(textField2);
		
		this.add(panel);
		setVisible(true);
	}
