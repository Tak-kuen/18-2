package _20181112URL;

import java.io.IOException;
import java.net.*;

public class WDB {
	public static void main(String[] args) {
		URL naver=null;
		URLConnection naverC=null;
		
		try {
			naver = new URL("https://www.naver.com");
			naverC = naver.openConnection();
				
		} catch (MalformedURLException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
