import javax.swing.*;
import java.awt.*;

class MyFrame1 extends JFrame{
	public MyFrame1() {
		setSize(600,450);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setTitle("title");
		
		JPanel panel = new JPanel();
		JLabel label = new JLabel();
		JButton bt = new JButton();
		panel.add(label);
		panel.add(button);
	}
}
