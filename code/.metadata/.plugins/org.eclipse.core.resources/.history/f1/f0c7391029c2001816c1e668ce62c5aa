import javax.swing.*;
import java.awt.*;

class MyPanel extends JPanel {
	@Override
	protected void paintComponent(Graphics g) {
		// TODO Auto-generated method stub
		super.paintComponent(g);
		g.drawString("STK", 200, 200);	//출력할 글자,x좌표,y좌표
		g.drawLine(200, 250, 300, 250);	//시작점 x,y좌표, 끝점 x,y좌표
		g.drawRect(0, 100, 100, 100);	//시작점(왼쪽위) x,y좌표, 가로, 세로
		g.fillRect(0, 200, 100, 100);
		g.setColor(Color.RED);
		g.fillRect(0, 300, 100, 100);
		g.drawRoundRect(100,200 , 100, 100, 40, 40);
		g.setColor(Color.BLUE);
		g.drawOval(200, 200, 100, 100);
	}
}


class MyFrame extends JFrame {
	public MyFrame() {
		setTitle("그래픽테스트");
		setSize(500,400);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
		
		//Panel을 생성하여 추가한다.
		MyPanel myPanel = new MyPanel();
		add(myPanel);
	}
}

public class WDB {
	public static void main(String[] args) {
		MyFrame myFrame = new MyFrame();
	}
	
}
