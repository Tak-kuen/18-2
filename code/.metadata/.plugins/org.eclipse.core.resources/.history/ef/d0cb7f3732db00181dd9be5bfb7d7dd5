package _20181029¾²·¹µå;

class Buffer {
	private int data;
	private boolean empty=true;
	
	public  int get() {
		while(empty) {
			try {
				wait();
			} catch (InterruptedException e) {
			}
		}
		empty=true;
		return data;
	}
	
	public  void put(int data) {
		while(!empty) {
			try {
				wait();
			} catch(InterruptedException e) {
			}
		}
		empty=false;
		this.data=data;
	}
}
