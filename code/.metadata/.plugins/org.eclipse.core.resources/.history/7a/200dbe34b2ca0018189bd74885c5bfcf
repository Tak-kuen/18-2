class MyPanel extends JPanel {
	BufferedImage img=null;
	
	public MyPanel() {
		setLayout(new BorderLayout());
		try {
			img = ImageIO.read(new File("C:\\Users\\bon320-14\\Desktop\\Files\\icon.jpg"));
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
