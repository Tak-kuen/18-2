import javax.swing.*;

//1. 리스너 인터페이스를 상속
//2. 버튼에 리스너 등록
//3. 리스너 이벤트 처리 메소드 구현

class MyFrame extends JFrame {
	public MyFrame() {
		setSize(300,600);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setTitle("색상변경 이벤트");
		setVisible(true);
		JPanel panel = new MyPanel();
		add(panel);
	}
}

public class WDB {
	
	public static void main(String[] args) {
		MyFrame myFrame= new MyFrame();
	}

}
