import javax.swing.*;

public class WDB {
	public static void main(String[] args) {
		//GUI프로그래밍을 위한 최상급 컨테이너 생성
		JFrame f = new JFrame("9ood");
		//컨테이너의 크기 설정
		f.setSize(600,450);
		//컨테이너 종료버튼 누를시 프로그램이 완전히 종료되게 하기
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		//컨테이너를 화면에 출력
		f.setVisible(true);
	}
}
